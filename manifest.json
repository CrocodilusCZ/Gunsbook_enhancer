{
    "manifest_version": 3,
    "name": "GunsBook Enhancer",
    "version": "1.0.5",
    "description": "Rozbalí příspěvky/komentáře, zvýrazní nejnovější komentář a vylepšuje zobrazení obrázků",
    "icons": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://gunsbook.com/*"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "author": "Redsnake"
  }